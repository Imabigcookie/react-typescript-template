{
  "name": "@imabigcookie/react-fsd-template",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Project template with React, Typescript, Vite and FSD",
  "engines": {
    "node": ">=18.15.0",
    "npm": ">=9.5.0"
  },
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "ci": "rm -rf node_modules && yarn install --frozen-lockfile",
    "dev": "vite dev -c vite/dev.config.ts",
    "build": "vite build -c vite/prod.config.ts",
    "lint": "run-p lint:*",
    "lint:types": "tsc --noEmit --skipLibCheck",
    "lint:js": "eslint src --ext .tsx --ext .ts",
    "lint:css": "stylelint src/**/*.css"
  },
  "author": "@ImaBigCookie",
  "license": "ISC",
  "dependencies": {
    "@tanstack/router": "^0.0.1-beta.139",
    "@tanstack/router-devtools": "^0.0.1-beta.139",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@types/node": "^20.4.8",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-boundaries": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-typescript-d-ts": "^1.0.0",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "5.1.6",
    "vite": "^4.4.8"
  }
}
